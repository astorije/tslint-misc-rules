    "detect-newline": "^2.1.0"
  },
  "peerDependencies": {
    "tslint": "^5.0.0",
    "typescript": ">=2.1 <=2.5"
  },
  "devDependencies": {
    "@types/node": "^7.0.28",
    "chokidar": "^1.6.1",
    "tslint": "^5.0.0",
